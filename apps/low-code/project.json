{
  "name": "low-code",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/low-code/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/vite:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/apps/low-code"
      },
      "configurations": {
        "development": {
          "mode": "development"
        },
        "production": {
          "mode": "production"
        }
      }
    },
    "serve": {
      "executor": "@nx/vite:dev-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "low-code:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "low-code:build:development",
          "hmr": true
        },
        "production": {
          "buildTarget": "low-code:build:production",
          "hmr": false
        }
      }
    },
    "preview": {
      "executor": "@nx/vite:preview-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "low-code:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "low-code:build:development"
        },
        "production": {
          "buildTarget": "low-code:build:production"
        }
      }
    },
    "test": {
      "executor": "@nx/vite:test",
      "outputs": ["{options.reportsDirectory}"],
      "options": {
        "passWithNoTests": true,
        "reportsDirectory": "../../coverage/apps/low-code"
      }
    }
  },
  "tags": [],
  "sideEffect": ["dayjs"]
}
